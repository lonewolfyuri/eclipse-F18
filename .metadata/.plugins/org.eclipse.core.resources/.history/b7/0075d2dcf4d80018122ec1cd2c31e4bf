from collections import defaultdict   # If you choose to use defaultdict  

# You might use these abbreviations in your code
# c  : caller
# r  : receiver
# t  : time
# ts : list of times


def all_parties(db : {str:{str:[int]}}) -> {str}:
    speakers, temp = {speaker for speaker in db}, set()
    for person in speakers: temp.update({speaker for speaker in db[person]})
    return speakers.union(temp)



def read_db(file : open) -> {str:{str:[int]}}:
    result = dict()
    for line in file:
        line_split = line.rstrip().split(':')
        caller, reciever, calls = line_split[0], line_split[1], line_split[2:]
        for item in range(len(calls)): calls[item] = int(calls[item])
        if caller not in result: result[caller] = defaultdict(list)
        result[caller][reciever].extend(calls)
    for item in result: result[item] = dict(result[item])
    return result



def talker_frequency (db : {str:{str:[int]}}) -> [(str,int)]:
    call_dict = defaultdict(int)
    for caller in db:
        for reciever in db[caller]:
            call_dict[caller] += len(db[caller][reciever])
            call_dict[reciever] += len(db[caller][reciever])
    return sorted(call_dict.items(), key= (lambda x: (-x[1], x[0])))       



def mutual_callers (db : {str:{str:[int]}}) -> {(str,str)}:
    result, temp = set(), set()
    for caller in db:
        for reciever in db[caller]:temp.add((caller, reciever))
    for caller, reciever in temp:
        if reciever in db and caller in db[reciever]: result.add(tuple(sorted((caller, reciever))))
    return result



def initiators (db : {str:{str:[int]}}) -> [str]:
    call_dict = dict()
    for caller in db:
        for reciever in db[caller]:
            if caller not in call_dict: call_dict[caller] = [0,0]
            if reciever not in call_dict: call_dict[reciever] = [0,0]
            call_dict[caller][0] += len(db[caller][reciever])
            call_dict[reciever][1] += len(db[caller][reciever])
    return sorted(call_dict.keys(), key= (lambda x: (-call_dict[x][0], call_dict[x][1])))



def matrix (db : {str:{str:[int]}}) -> [[int]]:
    speakers, temp, result = {speaker for speaker in db}, set(), []
    for person in speakers: temp.update({speaker for speaker in db[person]})
    speakers = sorted(speakers.union(temp))
    for _ in speakers:
        vals = []
        for _2 in speakers: vals.append(0)
        result.append(vals)
    for ndx in range(len(speakers)):
        for recipient in db[speakers[ndx]]:
            for convo in db[speakers[ndx]][recipient]:
                result[ndx][speakers.index(recipient)] += convo
                result[speakers.index(recipient)][ndx] += convo
    return result



